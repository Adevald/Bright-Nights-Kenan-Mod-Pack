msgstr[2] ""

#: .\monsters\mutant.json
msgid "half worm"
msgid_plural "half worms"
msgstr[0] ""
msgstr[1] ""
msgstr[2] ""

#. ~ Description for {'str': 'half worm'}
#: .\monsters\mutant.json
msgid "A squiggling severed portion of a wounded giant worm."
msgstr ""

#: .\monsters\mutant.json
msgid "swampass"
msgid_plural "swampasses"
msgstr[0] ""
msgstr[1] ""
msgstr[2] ""

#. ~ Description for {'str': 'swampass', 'str_pl': 'swampasses'}
#: .\monsters\mutant.json
